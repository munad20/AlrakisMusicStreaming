NNA </small></div> <!-- Artist -->\n  </div>\n</template>\n<script>\nexport default {\n  name: 'AlbumArt'\n}\n</script>\n<style scoped>\n  .parent-album {\n    /* GENERAL STYLING */\n    width: 18%;\n    height: 15em;\n    background: rgb(30, 30, 30);\n    position: fixed;\n    bottom: 0;\n    color: white;\n    justify-content: center;\n    align-items: center;\n    text-align: center;\n    text-transform: uppercase;\n    /* PULL INFRONT */\n    z-index: 10;\n    border-top: 0.1em solid #323232;\n\n    /* Parent create grid */\n    display: grid;\n     /* Repeat 1fr space for 3 columns */\n    grid-template-columns: repeat(3, 1fr);\n    /* Make 4 rows with 1fr space */\n    grid-template-rows: repeat(4, 1fr);\n    /* No gaps between columns or rows */\n    grid-column-gap: 0px;\n    grid-row-gap: 0px;\n  }\n  /* Belonging to parent display:grid. grid-area means Row start, column start, row end, column end */\n    .div2-album { grid-area: 1 / 1 / 2 / 4; }\n    .div3-album { grid-area: 2 / 1 / 3 / 4; }\n    .div4-album { \n        align-self: flex-end;\n        grid-area: 3 / 1 / 4 / 4; }\n    .div5-album { \n        align-self: baseline;\n        grid-area: 4 / 1 / 5 / 4; }\n\n  /* TYPOGRAPHY */\n  small{\n      align-content: space-around;\n      color: grey;\n      font-size: .4em;\n  }\n  p{\n      font-size: .5em;\n  }\n  h2{\n      font-size: .75em;\n  }\n  /* IMAGE */\n  img{\n      max-width: 40%;\n      max-height: 40%;\n  }\n</style>\n"]}]}